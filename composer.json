{
    "name": "fp/fumocker",
    "type": "library",
    "description": "A php function mocker",
    "keywords": ["unit test", "function mock", "phpunit"],
    "homepage": "http://github.com/formapro/Fumocker",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Tyukov",
            "email": "brainreflex@gmail.com"
        },
        {
            "name": "Maksim Kotlyar",
            "email": "kotlyar.maksim@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Fumocker\\": "src/Fumocker"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fumocker\\Tests\\": "tests/Fumocker"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
